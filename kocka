import random
a=0
b=0
c=0
d=0
e=0
f=0
for i in range (1, 51):
    nah = random.randrange(1,7)
    if nah ==1:
        a = a+1
    if nah ==2:
        b = b+1
    if nah ==3:
        c = c+1
    if nah == 4:
        d = d+1
    if nah == 5:
        e = e+1
    if nah == 6:
        f = f+1
print("1:"+ str(a))
print("2:"+str(b))
print("3:"+str(c))
print("4:"+str(d))
print("5:"+str(e))
print("6:"+str(f))

