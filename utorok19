#11
def permutacia():
    ret = "0123"
    for i in ret:
        for j in ret:
            for k in ret:
                for l in ret:
                    print(i,j,k,l)
permutacia()

#12
def permutacia():
    ret = "ahoj"
    for i in ret:
        for j in ret:
            for k in ret:
                for l in ret:
                    print(i,j,k,l)
permutacia()
