def pocet_sam_sp(a):
    samohlasky = "aeiouy"
    spoluhlasky = "qwrtpsdfghjklxcvbnmz"
    pocetsam = 0
    pocetsp = 0
    for i in a:
        if i in samohlasky:
            pocetsam +=1
        if i in spoluhlasky:
            pocetsp +=1
    print(pocetsam)
    print(pocetsp)
pocet_sam_sp("ahoj")

